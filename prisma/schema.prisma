generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  url       String
  productId String   @map("product_id")
  storeType String   @map("store_type")
  createdAt DateTime @default(now()) @map("created_at")

  @@map("products")
}